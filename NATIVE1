 (                                                                      00020000
  IN  PEMPNO        CHAR(6)                                             00030000
 ,OUT PFIRSTNME     VARCHAR(12)                                         00040000
 ,OUT PMIDINIT      CHAR(1)                                             00050000
 ,OUT PLASTNAME     VARCHAR(15)                                         00060000
 ,OUT PWORKDEPT     CHAR(3)                                             00070000
