 LANGUAGE SQL                                                           00221001
                                                                        00221101
  BEGIN                                                                 00221201
    DECLARE SQLCODE INTEGER;                                            00221301
    DECLARE SQLSTATE CHAR(5);                                           00222001
    SELECT                                                              00223001
       FIRSTNME                                                         00224001
