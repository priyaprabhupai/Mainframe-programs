 ,OUT PSALARY       DEC(9,2)                                            00090000
 ,OUT PSQLCODE      INTEGER                                             00100000
 ,OUT PSQLSTATE     CHAR(5)                                             00110000
 ,OUT PSQLERRMC     VARCHAR(250)                                        00120000
 )                                                                      00130000
 RESULT SETS 0                                                          00140000
 MODIFIES SQL DATA                                                      00150000
 NO DBINFO                                                              00160000
 WLM ENVIRONMENT DB2GDES1                                               00170000
 STAY RESIDENT NO                                                       00180000
 COLLID DEVL7083                                                        00190000
 PROGRAM TYPE MAIN                                                      00200001
